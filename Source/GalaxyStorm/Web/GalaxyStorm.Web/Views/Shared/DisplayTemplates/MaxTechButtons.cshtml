@model GalaxyStorm.ViewModels.Technologies.TechnologyViewModel                    

<div class="row">
    <div class="col-md-4 col-md-offset-4 tech-level-button">
        <button class="btn btn-block btn-success">
            <h5>
                <i class="@ViewData["glyph"]"> </i>
                <br/>
                @ViewData["sign"] @Model.Modifiers[Model.Level]@ViewData["mod"]
            </h5>
        </button>
    </div>
</div>
<br />
<p><strong class="text-success">Max Level Reached!</strong></p>
<span>@Model.Description</span>
                    
